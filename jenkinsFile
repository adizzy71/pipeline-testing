pipeline {
    agent any
    stages {
        stage("scm checkout") {



            steps {
                def scmVars = checkout scm
                def commitHash = scmVars.GIT_COMMIT
                echo "step 1"
                echo "step 2"
                echo "The commit record is $env.GIT_COMMIT"
                bat 'set'
            }

        }
    }
}
